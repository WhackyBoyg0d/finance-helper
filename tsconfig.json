{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@data/*": ["data/*"]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es2016",       
    "jsx": "react",             
    "module": "commonjs", 
    "forceConsistentCasingInFileNames": true, 
    "strict": true,
    "skipLibCheck": true 
  },
  "include": ["./src/**/*.tsx","assets","./src/**/*.ts"]
}
